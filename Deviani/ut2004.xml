<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Name>ut2004</Name>
  <Date>2021-02-28</Date>
  <WebUI>https://[IP]:[PORT:10080]</WebUI>
  <Description>
    Docker container to run a ut2004 Server 
  </Description>
  <Overview>
   Docker container to run a ut2004 Server 
  </Overview>
  <Support></Support>
  <Registry>https://hub.docker.com/r/reflectivecode/ut2004/</Registry>
  <GitHub>https://github.com/reflectivecode/ut2004</GitHub>
  <Repository>reflectivecode/ut2004</Repository>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>10080</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
	  <Port>
        <HostPort>10777</HostPort>
        <ContainerPort>7777</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
	  <Port>
        <HostPort>10778</HostPort>
        <ContainerPort>7778</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data></Data>
  <Environment>
    <Variable>
      <Value>[Engine.AccessControl];AdminPassword=password123;[UWeb.WebServer];bEnabled=True</Value>
      <Name>CONFIG_1</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>CTF-FACECLASSIC?game=XGame.xCTFGame</Value>
      <Name>UT2004_CMD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Banner></Banner>
  <Icon>https://i.imgur.com/rurkmTS.jpg</Icon>
  <ExtraParams></ExtraParams>
</Containers>