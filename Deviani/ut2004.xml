<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Name>ut2004</Name>
  <Date>2021-02-28</Date>
  <WebUI>http://[IP]:[PORT:7080]</WebUI>
  <Description>
    Docker container to run a ut2004 Server 
  </Description>
  <Overview>
   Default login is admin:admin1 
  </Overview>
  <Support></Support>
  <Registry>https://hub.docker.com/r/captbrick/ut2004</Registry>
  <GitHub>https://github.com/RytisLT/docker-ut2004</GitHub>
  <Repository>rytislt/docker-ut2004</Repository>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>7080</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
	  <Port>
        <HostPort>7777</HostPort>
        <ContainerPort>7777</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
	  <Port>
        <HostPort>7778</HostPort>
        <ContainerPort>7778</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
	  <Port>
        <HostPort>7787</HostPort>
        <ContainerPort>7787</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
	  <Port>
        <HostPort>28902</HostPort>
        <ContainerPort>28902</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data></Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Banner></Banner>
  <Icon>https://i.imgur.com/rurkmTS.jpg</Icon>
  <ExtraParams></ExtraParams>
</Containers>